# bc_simple_rules.krb

Do_you_have_an_Urinary_tract_infection
    use what_to_bring(you_have_an_Urinary_tract_infection)
    when
        questions3.Is_your_temperature_higher_than_39(True)
        questions3.Is_the_duration_of_the_fever_more_than_3_days(True)
        questions3.Do_you_fear_the_bathroom_a_lot(True)
        questions3.Do_you_have_discharge_with_urine(True)
        questions3.Do_you_have_blood_in_your_urine(True)
